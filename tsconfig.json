{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "ionic-context-menu": [
        "dist/ionic-context-menu"
      ],
      "ionic-context-menu/*": [
        "dist/ionic-context-menu/*"
      ]
    },
    "baseUrl": "./"
  }
}